class node:
    def __init__(self,data):
        self.data=data
        self.left=None
        self.right=None
main=node("main center")
node_a=node("a")
node_b=node("b")
node_c=node("c")
node_d=node("d")
node_f=node("e")
node_g=node("f")
main.left=node_a
main.right=node_b
node_a.left=node_c
node_a.right=node_d
node_b.left=node_f
node_b.right=node_g
print("main node data:",main.data)
print("value: ",main.left.right.data)
