def binary_search():
    l=eval(input("enter: "))
    target=int(input("enter target:"))#index=4
    left=0
    right=len(l)-1
    while(left<=right):
            min=(left+right)//2
            if l[min]==target:
                print("value ",target,"at index",min)
                return("found")
            elif l[min]<target:
                left=min+1
            else:
               if  l[min]>target:
                   right=min-1
print(binary_search())                
